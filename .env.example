# VerimutFS Node Configuration

# Listen port for P2P connections (0 = random port)
LISTEN_PORT=0

# HTTP API server port
API_PORT=3001

# Bootstrap peers (comma-separated multiaddrs)
# Add your network's bootstrap nodes here
BOOTSTRAP_PEERS=

# Data storage directory
DATA_DIR=./verimut-data

# Enable verbose logging
VERBOSE=false
